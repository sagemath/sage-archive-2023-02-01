#!/usr/bin/env bash

cd $SAGE_ROOT/doc/


grep -i "$@" *.html  2>/dev/null
grep -i "$@" */*.html 2>/dev/null
grep -i "$@" */*/*.html  2>/dev/null
grep -i "$@" */*/*/*.html  2>/dev/null
grep -i "$@" */*/*/*/*.html  2>/dev/null
grep -i "$@" */*/*/*/*/*.html  2>/dev/null
grep -i "$@" */*/*/*/*/*/*.html  2>/dev/null
grep -i "$@" */*/*/*/*/*/*/*.html  2>/dev/null
grep -i "$@" */*/*/*/*/*/*/*/*.html  2>/dev/null

