+            sage: Permutation([]).binary_search_tree()
+            .
+        """
+        from sage.combinat.binary_tree import LabelledBinaryTree as LBT
+        res = LBT(None)
+        if left_to_right:
+            gen = self
+        else:
+            gen = self[::-1]
+        for i in gen:
+            res = res.binary_search_insert(i)
+        return res
